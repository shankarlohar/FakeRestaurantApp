<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shankarlohar.fakerestaurantandroidapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FakeRestaurantAndroidApp">
        <activity
            android:name=".DesertActivity"
            android:exported="true"
            android:label="DesertActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".mainCourseActivity"
            android:exported="true"
            android:label="Main Course Options"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StarterActivity"
            android:exported="true"
            android:label="Starters Options"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>